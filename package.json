  "dependencies": {
    "algoliasearch": "^4.5.1"
  },
  "devDependencies": {
    "@freecodecamp/eslint-config": "2.0.2",
    "babel-eslint": "10.1.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "serverless": "3.21.0",
    "serverless-offline": "9.2.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
